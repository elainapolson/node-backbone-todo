doctype html
html
  head
    title= title + ' | ' + appname
    link(rel="stylesheet", href="/stylesheets/style.css")
    link(rel="stylesheet", href="/bootstrap/bootstrap.css")
    link(rel="stylesheet", href="/stylesheets/main.css")

  body
    .container
      .navbar.navbar-default
        .container
          .navbar-header
            a.navbar-brand(href='/')= appname
      .alert.alert-dismissable
      h1= title
      p Welcome to Elaina's Todo App
      block content
      
  script(src='/javascripts/jquery.js', type="text/javascript")
  script(src="/js/lib/jquery.min.js", type="text/javascript")
  script(src='/javascripts/main.js', type="text/javascript")
  script(src='/js/lib/underscore-min.js', type="text/javascript")
  script(src='/js/lib/backbone-min.js', type="text/javascript")
  script(src='/js/models/book.js', type="text/javascript")
  script(src='/js/collections/library.js', type="text/javascript")
  script(src='/js/views/book.js', type="text/javascript")
  script(src='/js/views/library.js', type="text/javascript")
  script(src='js/app.js', type="text/javascript")